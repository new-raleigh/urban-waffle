{
  "name": "posts-backend",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "echo ok",
    "upgrade": "yarn i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "yarn i cdktf@next cdktf-cli@next",
    "deploy:dev": "cdktf deploy '*-dev' && yarn deploy:frontend",
    "deploy:prod": "cdktf deploy '*-prod' && yarn deploy:frontend",
    "deploy:frontend": "yarn workspace frontend run deploy",
    "destroy:dev": "yarn destroy:frontend && cdktf destroy '*-dev'",
    "destroy:prod": "yarn destroy:frontend && cdktf destroy '*-prod'",
    "destroy:frontend": "yarn workspace frontend run destroy",
    "plan": "cdktf diff",
    "deploy": "cdktf deploy --auto-approve",
    "destroy": "cdktf destroy --auto-approve",
    "deploy:s3": "cd frontend/code; yarn run deploy"
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "@cdktf/provider-aws": "^11.0.7",
    "@cdktf/provider-local": "^4.0.0",
    "cdktf": "^0.14.3",
    "constructs": "^10.1.191",
    "cron-time-generator": "^1.3.0",
    "esbuild": "^0.16.7"
  },
  "devDependencies": {
    "@cdktf/provider-random": "^4.0.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.11.15",
    "cdktf-cli": "^0.14.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "workspaces": {
    "packages": [
      ".",
      "posts/api/lambda",
      "frontend/code"
    ]
  }
}
